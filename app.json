{
  "expo": {
    "name": "Number Game",
    "slug": "number-game",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/ios-light.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icons/splash-icon-light.png",
      "resizeMode": "contain",
      "backgroundColor": "#fcd35c"
    },
    "ios": {
      "ios.buildNumber": "1.0.0",
      "supportsTablet": true,
      "icon": {
        "light": "./assets/images/icons/ios-light.png",
        "dark": "./assets/images/icons/ios-dark.png"
      }
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/adaptive-icon.png",
        "backgroundColor": "#fcd35c"
      },
      "edgeToEdgeEnabled": true,
      "package": "dev.expo.numbergame"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "7ab7accc-2839-4aff-bc74-a160f1971643"
      }
    }
  }
}
